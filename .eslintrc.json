{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": "airbnb",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "never"
    ],
    "no-console": 0,
    "camelcase": "off",
    "no-useless-concat":"off",
    "prefer-template":"off",
    "no-restricted-syntax":"off",
    "no-underscore-dangle":"off",
    "no-path-concat":"off",
    "no-lone-blocks":"warn"
  }
}
